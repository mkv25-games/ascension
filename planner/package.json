{
  "name": "ascension-planner",
  "version": "1.0.0",
  "description": "Ascension Tile Planner",
  "main": "main.js",
  "scripts": {
    "start": "node main.js && npm run-script generate:website",
    "generate:website": "hag generate > generate.log",
    "generate:areas": "hag generate *.json -i instructions-generated",
    "upload:text": "cd build/website && node ../../scripts/upload-text.js && cd ../../",
    "upload:all": "cd build/website && node ../../scripts/upload-all.js && cd ../../",
    "import:area": "node scripts/add-area-layout.js"
  },
  "author": "Markavian",
  "license": "ISC",
  "dependencies": {
    "promise-path": "^1.2.2"
  },
  "devDependencies": {
    "ftp-client": "^0.2.2",
    "get-pixels": "^3.3.0"
  }
}
